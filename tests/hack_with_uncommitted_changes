# setup
echo "#" > README.md
git add README.md >& /dev/null

# try
output="$(scripts/hack.sh -t 2>&1)"

# cleanup
git checkout HEAD -- README.md

# verify
result="$(echo ${output} | grep 'test complete')"

# report
if [ "$result" == "" ]
then
	printf "%-50s PASS\n" "$0"
	exit 0
else
	printf "%-50s FAIL\n" "$0"
	exit 1
fi

